<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Beneficiary Validations" verbose="1" parallel="tests">

    <test name="Validate the Beneficiaries Quebec Add Test English">
        <parameter name="data-set" value="data/beneficiaries_quebec_data_set_en.xml"/>
        <classes>
            <class name="com.canadalife.automation.grsoe.sprinttests.BeneficiariesQuebecRevocableTest"/>
        </classes>
    </test>

    <test name="Validate the Beneficiaries Quebec Add Test French">
        <parameter name="data-set" value="data/beneficiaries_quebec_data_set_fr.xml"/>
        <classes>
            <class name="com.canadalife.automation.grsoe.sprinttests.BeneficiariesQuebecRevocableTest"/>
        </classes>
    </test>

    <test name="Validate the Accordion Investment Test English">
        <parameter name="data-set" value="data/investment_data_set_en_RPP.xml"/>
        <classes>
            <class name="com.canadalife.automation.grsoe.sprinttests.InvestmentsAccordionTest"/>
        </classes>
    </test>

    <test name="Validate the Accordion Investment Test French">
        <parameter name="data-set" value="data/investment_data_set_fr_RPP.xml"/>
        <classes>
            <class name="com.canadalife.automation.grsoe.sprinttests.InvestmentsAccordionTest"/>
        </classes>
    </test>

</suite>